  "bugs": {
    "url": "https://github.com/hesusruiz/PrivacyWallet/issues"
  },
  "homepage": "https://github.com/hesusruiz/PrivacyWallet#readme",
  "dependencies": {
    "lit": "^2.0.0-rc.2"
  },
  "devDependencies": {
    "@web/dev-server": "^0.1.17",
    "eslint": "^7.28.0",
    "rollup": "^2.52.1"
  }
